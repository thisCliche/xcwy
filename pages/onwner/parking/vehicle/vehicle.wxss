/* pages/onwner/payment/paymentList/paymentList.wxss */
.vehicle{
  padding: 28rpx 20rpx;
}
.item{
  display: flex;
  justify-content: space-between;
}
.title{
  font-size: 36rpx;
}
.des{
  font-size: 32rpx;
  margin: 24rpx 0 0 0rpx;
}
.left{
  padding: 34rpx 12rpx;
  display: flex;
  align-items: center;
}
.right{
  /* padding: 34rpx 0; */
  display: flex;
  align-items: center;
  width: 140rpx;
  box-sizing: border-box;
  border-left: 1rpx solid rgba(150, 150, 150, 0.3);
}
.btn{
  color: #444444;
  font-size: 30rpx;
  padding: 0 30rpx;
}
.favicon{
  padding: 0 24rpx;
}
.favicon image{
  width: 66rpx;
  height: 56rpx;
}
.warning{
  color: #F54235;
}
.btns{
  color: #FFA467;
  font-size: 50rpx;
  text-align: center;
  margin-top: 96rpx;
}
.singlePayment{
  padding: 60rpx 30rpx;
  display: flex;
  justify-content: space-between;
}
.singleL{
  font-size: 36rpx;
}
.singleR{
  font-size: 32rpx;
}